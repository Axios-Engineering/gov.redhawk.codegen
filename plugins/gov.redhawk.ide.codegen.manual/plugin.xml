<plugin>
   <extension
         point="gov.redhawk.ide.codegen.codegens">
      <codegen
            autoGenerate="false"
            class="gov.redhawk.ide.codegen.manual.ManualGenerator"
            id="gov.redhawk.ide.codegen.manual.ManualGenerator"
            language="*"
            name="Manual Generator"
            notDefaultable="false"
            priority="low">
         <description>
            The User expects to do all of the code from scratch, hence this will not generate any code.
         </description>
         <operatingSystem
               default="true"
               os="Linux">
         </operatingSystem>
         <componenttype
               type="resource">
         </componenttype>
         <componenttype
               type="device">
         </componenttype>
         <componenttype
               type="service">
         </componenttype>
      </codegen>
   </extension>


</plugin>
